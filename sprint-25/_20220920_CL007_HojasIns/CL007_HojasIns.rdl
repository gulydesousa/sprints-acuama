      <Query>
        <DataSourceName>ConexionBD</DataSourceName>
        <CommandText>DECLARE @consumoPromedio INT
SET @consumoPromedio = NULL

--Guardar el contenido de la select en una tabla temporal #
SELECT *, NULL AS consumoPromedio
		 INTO #TempTable
		 FROM (SELECT  facLecInlCod
					,facConsumoFactura
					,facctrcod
					,facpercod
					,faclecant
					,faclecantfec
					,facLectorEplCod
					,facLectorCttCod
					,faclote
					,ctremplaza
					,ctrPagNom
					,ctrTitNom
					,ctravisolector
					,ctrbatfila
					,ctrbatcolum
					,ctrRuta1
					,ctrRuta2
					,ctrRuta3
					,ctrRuta4
					,ctrRuta5
					,ctrRuta6
					,inmdireccion
					,eplcod
					,eplnom
					,cttnom
					,conNumSerie
					,conDiametro
					,inldes
				FROM facturas 
					INNER JOIN contratos c ON ctrcod=facctrcod AND ctrversion = facctrversion
					INNER JOIN inmuebles ON ctrinmcod=inmcod --la direccion de suministro es la del imnueble
					LEFT JOIN empleados ON eplcod=facLectorEplCod AND eplcttcod=facLectorCttCod
					LEFT JOIN contratistas ON cttcod = facLectorCttCod
					LEFT JOIN fContratos_ContadoresInstalados(NULL) ON ctcCtr = ctrCod
					LEFT JOIN incilec ON inlcod = facInsInlCod
				WHERE
					(facZonCod &gt;=@zonaD or @zonaD IS NULL)
					AND (facZonCod &lt;=@zonaH or @zonaH IS NULL)
					AND facPerCod=@periodo
					AND (facInspeccion = @Inspeccion OR (@Inspeccion = 1 AND facConsumoFactura &lt; 0) OR @Inspeccion IS NULL)
					AND ((facLecLector is null AND facLecInspector is null AND facInsInlCod is null AND facLecInlCod is null AND @sinLeer=1) or @sinLeer=0)

					AND (facconsumofactura &gt;= @consumoD or @consumoD IS NULL)
					AND (facconsumofactura &lt;= @consumoH or @consumoH IS NULL)

					AND (facLecInlCod &gt;= @ObsLecD or @ObsLecD IS NULL)
					AND (facLecInlCod &lt;= @ObsLecH or @ObsLecH IS NULL)

					AND (facLote &gt;=@loteD or  @loteD IS NULL)
					AND (facLote &lt;=@loteH or  @loteH IS NULL)

					AND (@tieneObsLec IS NULL OR @tieneObsLec = 1 OR (@tieneObsLec = 0 AND facLecInlCod IS NULL))
					AND (@tieneObsIns IS NULL OR @tieneObsIns = 1 OR (@tieneObsIns = 0 AND facInsInlCod IS NULL))

					AND (facInsInlCod &gt;= @ObsInsD or @ObsInsD IS NULL)
					AND (facInsInlCod &lt;= @ObsInsH or @ObsInsH IS NULL)

					AND (@difMinLecLectorYAnt IS NULL OR abs(facLecLector - facLecAnt) &gt;= @difMinLecLectorYAnt)

					AND (@incluirContratosSinServicioMedidoActivo = 1 OR
						 EXISTS (SELECT ctsCtrCod
       								   FROM contratoServicio
								   INNER JOIN servicios ON svcCod = ctsSrv
								   WHERE contratoServicio.ctsCtrCod = c.ctrCod AND --Unión con la SELECT exterior (contratos)
										 svcTipo = 'M'  AND --Servicio Medido 'M'
									 (dbo.GetAcuamaDate() &lt; ctsFecBaj OR ctsFecBaj IS NULL) --Sólo servicios que estén de alta
							)
						)
					AND facFechaRectif IS NULL
			 
			) AS OriginalTable
			
--Solo si se específica algún porcentaje se recorrerá la tabla y se insertará el consumo promedio en el campo correspondiente
IF @pcjeDisminucionMinCns IS NOT NULL OR @pcjeAumentoMinCns IS NOT NULL
	BEGIN
		--CURSOR
		DECLARE @facContrato AS INT
		DECLARE @facPeriodo AS VARCHAR(6) 
		
		DECLARE c1 CURSOR FOR
		SELECT facctrcod, facpercod
			   FROM #TempTable
		OPEN c1
		
		FETCH NEXT FROM c1 INTO @facContrato, @facPeriodo
		WHILE @@FETCH_STATUS = 0
		BEGIN
			--Obtener el consumo promedio
			EXEC Facturas_CalculoConsumoPromedio @facContrato, @facPeriodo, 0, @consumoPromedio OUTPUT
			--Almacenar en la tabla #TempTable para posteriormente condicionar en la consulta final
			UPDATE #TempTable 
				   SET consumoPromedio = @consumoPromedio
				   WHERE facctrcod = @facContrato AND 
					 facpercod = @facPeriodo
						 
			FETCH NEXT FROM c1 INTO @facContrato, @facPeriodo
		END
		CLOSE c1
		DEALLOCATE c1
	END

SELECT * FROM #TempTable 
	 WHERE(@pcjeDisminucionMinCns IS NOT NULL AND facConsumoFactura &lt; consumoPromedio * (1 - @pcjeDisminucionMinCns / 100.0)) OR
	      (@pcjeAumentoMinCns IS NOT NULL AND facConsumoFactura &gt; consumoPromedio * (1 + @pcjeAumentoMinCns / 100.0)) OR 											      (@pcjeDisminucionMinCns IS NULL AND @pcjeAumentoMinCns IS NULL)
ORDER BY faclote,  REPLICATE('0',10-LEN(ISNULL(ctrRuta1,''))) + ISNULL(ctrRuta1,'') 
		  +REPLICATE('0',10-LEN(ISNULL(ctrRuta2,''))) + ISNULL(ctrRuta2,'')
		  +REPLICATE('0',10-LEN(ISNULL(ctrRuta3,''))) + ISNULL(ctrRuta3,'')
		  +REPLICATE('0',10-LEN(ISNULL(ctrRuta4,''))) + ISNULL(ctrRuta4,'')
		  +REPLICATE('0',10-LEN(ISNULL(ctrRuta5,''))) + ISNULL(ctrRuta5,'')
		  +REPLICATE('0',10-LEN(ISNULL(ctrRuta6,''))) + ISNULL(ctrRuta6,'')


DROP TABLE #TempTable</CommandText>
