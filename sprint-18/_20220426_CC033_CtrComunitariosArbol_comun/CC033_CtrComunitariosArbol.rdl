    <DataSet Name="dsComunitarios">
      <Query>
        <DataSourceName>ConexionBD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@contrato">
            <Value>=Parameters!contrato.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@zonaD">
            <Value>=Parameters!zonaD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@zonaH">
            <Value>=Parameters!zonaH.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@periodoD">
            <Value>=Parameters!periodoD.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@periodoH">
            <Value>=Parameters!periodoH.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@soloCnsFinalDifAlGeneral">
            <Value>=Parameters!soloCnsFinalDifAlGeneral.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@negativos">
            <Value>=Parameters!negativos.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @servicioAgua AS INT = (SELECT pgsvalor FROM parametros WHERE pgsclave='SERVICIO_AGUA')

DECLARE @MCalculoDescontar AS INT = 1
DECLARE @MCalculoRepartir AS INT = 2

DECLARE @raiz as int = 0

IF @contrato &gt; 0 BEGIN
	exec Contratos_ObtenerRaiz @contrato, @raiz	output
	IF @raiz = 0 GOTO FIN
END

;WITH CTE(ctrCod, ctrVersion, ctrComunitario, nivel, orden) AS (

    --Caso base
    SELECT ctrCod, ctrversion, ctrComunitario, 0, CAST(ctrCod AS VARCHAR(1000)) AS orden
    FROM contratos c0
	WHERE c0.ctrfecanu is null and c0.ctrComunitario IS NULL
		AND (@zonaD IS NULL OR c0.ctrzoncod&gt;=@zonaD)
		AND (@zonaH IS NULL OR c0.ctrzoncod&lt;=@zonaH)
		
		AND EXISTS(select ctrcod from contratos c3 where c3.ctrComunitario = c0.ctrcod)
		AND (@raiz = 0 OR (@raiz = c0.ctrcod))
    
    UNION ALL
    
    --Caso recursivo
    SELECT c.ctrCod, c.ctrversion, c.ctrComunitario, nivel + 1, CAST(orden + '/' + CAST(c.ctrCod AS VARCHAR) AS VARCHAR(1000)) AS orden
    FROM contratos c INNER JOIN CTE ON c.ctrComunitario = CTE.ctrCod
    WHERE ctrfecanu IS NULL
)

SELECT c.ctrCod, c.ctrComunitario, nivel,
	ISNULL(facConsumoFactura,0) as facConsumoFactura,
	(CASE WHEN (ctrCalculoComunitario IS NULL OR ctrCalculoComunitario = @MCalculoRepartir OR (ctrCalculoComunitario = @MCalculoDescontar AND 
	       EXISTS(SELECT fclFacCod FROM faclin 
		      WHERE fclFacCod = facCod AND 
			    fclFacPerCod = facPerCod AND 
			    fclFacCtrCod = facCtrCod AND 
			    fclFacVersion = facVersion AND 
			    fclTrfSvCod = @servicioAgua))) THEN ISNULL(facConsumoFactura, 0) ELSE NULL END) AS facConsumoFacturaIncluidos, 
	(CASE WHEN (EXISTS(SELECT fclFacCod FROM faclin
		      WHERE fclFacCod = facCod AND 
			    fclFacPerCod = facPerCod AND 
			    fclFacCtrCod = facCtrCod AND 
			    fclFacVersion = facVersion AND 
			    fclTrfSvCod = @servicioAgua)) THEN 1 ELSE 0 END) AS existeSrvAguaFacturado,
	ISNULL(facCnsComunitario,0) as facCnsComunitario,
	ISNULL(facLecAnt,0) as facLecAnt,
	ISNULL(facLecAct,0) as facLecAct,
	facCnsFinal,
	ctrCalculoComunitario, ctrTitNom, ctrinmcod, inmdireccion,
	(SELECT TOP 1 fclUnidades FROM faclin
		      WHERE fclFacCod = facCod AND 
			    fclFacPerCod = facPerCod AND 
			    fclFacCtrCod = facCtrCod AND 
			    fclFacVersion = facVersion AND 
			    fclTrfSvCod = @servicioAgua) AS udsAgua,
ctrRuta1, ctrRuta2, ctrRuta3, ctrRuta4, ctrRuta5, ctrRuta6,
(SELECT COUNT(*)
	FROM facturas fSub
	INNER JOIN contratos cSub ON cSub.ctrCod = fSub.facCtrCod AND cSub.ctrVersion = fSub.facCtrVersion
	WHERE fSub.facPerCod = f.facPerCod AND
		fSub.facCtrCod = f.facCtrCod AND 
		fSub.facFechaRectif IS NULL AND 	
		EXISTS(SELECT ctrcod 
			      FROM contratos c1 
			      WHERE c1.ctrComunitario=cSub.ctrcod AND 
				    c1.ctrfecanu IS NULL
		       )	             
) AS EsPadre,
(SELECT COUNT(*)
	FROM facturas fSub
	INNER JOIN contratos cSub ON cSub.ctrCod = fSub.facCtrCod AND cSub.ctrVersion = fSub.facCtrVersion
	WHERE fSub.facPerCod = f.facPerCod AND
		fSub.facCtrCod = f.facCtrCod AND 
		fSub.facFechaRectif IS NULL AND 	
		EXISTS(SELECT ctrcod 
			      FROM contratos c1 
			      WHERE c1.ctrComunitario=cSub.ctrcod AND 
				    c1.ctrfecanu IS NULL
		       ) AND
		ISNULL(fSub.facCnsFinal,0) &lt;&gt; ISNULL(fSub.facCnsComunitario,0) 	             
) AS EsDesfase,
--Devuelve un 1 si tiene al menos un servicio por contrato activo, un 0 en caso contrario.
CASE WHEN EXISTS(SELECT ctsctrcod
			FROM contratoServicio
			WHERE ctsctrcod = c.ctrcod AND
			     (dbo.GetAcuamaDate() &lt; ctsFecBaj OR ctsFecBaj IS NULL)
		) 
THEN 1 ELSE 0 END AS serviciosActivos
, F.facPerCod
FROM CTE --Si se necesitan muchos más datos de contratos: 
INNER JOIN CONTRATOS c ON c.ctrCod = CTE.ctrCod AND c.ctrVersion = CTE.ctrVersion
inner join inmuebles on inmcod = c.ctrinmcod
left join facturas f on facCtrCod = CTE.ctrcod and facFechaRectif is null
WHERE
((@periodoD IS NULL OR facPerCod&gt;=@periodoD) AND (@periodoH IS NULL OR facPerCod&lt;=@periodoH)) AND 
	(@negativos IS NULL OR 
        (@negativos = 0 AND facCnsFinal &gt;= facCnsComunitario) OR 
        (@negativos = 1 AND facCnsFinal &lt; facCnsComunitario))
	AND 
	(@soloCnsFinalDifAlGeneral = 0 OR 
	(@soloCnsFinalDifAlGeneral = 1 AND facCnsFinal &lt;&gt; facCnsComunitario))
--ORDER BY orden, ctrCod
ORDER BY ISNULL(C.ctrComunitario, C.ctrCod), F.facPerCod, nivel, C.ctrCod 
FIN:</CommandText>
