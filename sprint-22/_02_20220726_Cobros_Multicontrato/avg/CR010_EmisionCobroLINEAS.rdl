    <DataSet Name="dsEmisionCobroLINEAS">
      <Query>
        <DataSourceName>ConexionBD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sociedad">
            <Value>=Parameters!sociedad.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@puntopago">
            <Value>=Parameters!puntopago.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@numcobro">
            <Value>=Parameters!numcobro.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @tablaAuxiliar as 
		TABLE (
		codigo int, 
		periodo  varchar(6), 
		cblImporte  money, 
		cobctr int, 
		SerieFactura  int, 
		NumeroFactura  VARCHAR(20), 
		FechaFactura datetime, 
		ImporteFactura money,
		pdte money,

		primary key(codigo, periodo,SerieFactura,numeroFactura,cblImporte,FechaFactura,cobctr)		
)

DECLARE @total as int    
DECLARE @codigo as int
DECLARE @periodo as varchar(6)
DECLARE @totCob as money
DECLARE @impFac as money
DECLARE @impCob as money
DECLARE @pdte as money
DECLARE @cobCtr as int

-- Insertamos las lineas del cobro
    INSERT into @tablaAuxiliar 
		select cblfaccod, cblPer, cblImporte, cobctr, facSercod, facNumero, facfecha as FechaFactura, sum(fcltotal), 0
                                from coblin
                                inner join cobros on cobScd = cblScd and cobPpag = cblPpag and cobNum = cblNum
                                inner join facturas on facCod = cblFacCod and facPerCod = cblPer and facCtrCod = cobctr and 
                                           facVersion = (select max(facversion) from facturas where facCod = cblFacCod and facPercod = cblPer and facCtrCod = cobCtr and facFecReg &lt;= cobFecReg)
                                inner join faclin on fclFacCod = cblFacCod and fclFacPerCod = cblPer and fclFacCtrCod = cobctr and fclFacVersion = facVersion
                                where 
                                cblScd=@sociedad and 
                                cblPpag=@puntopago and 
                                cblNum=@numcobro and
				(fclFecLiq IS NULL AND fclUsrLiq IS NULL)
                                group by cblfaccod, cblper, cblImporte, cobctr, facSerCod, facNumero, facfecha

    SET @total=0
    select @total=count(*) from @tablaAuxiliar
      
-- Obtenemos el total cobrado de cada periodo


    DECLARE cCobros CURSOR FOR
           Select codigo, periodo, cobCtr, ImporteFactura, cblImporte from @tablaAuxiliar
    OPEN cCobros
    FETCH NEXT FROM cCobros
    INTO @codigo, @periodo, @cobCtr, @impFac, @impCob
    WHILE @@FETCH_STATUS = 0
    BEGIN

        SET @totCob = 0
        SET @pdte = 0
        select @totCob= ISNULL(sum(cblImporte), 0) from coblin
                   inner join cobros on cobScd = cblScd and cobPpag = cblPpag and cobNum = cblNum and cobCtr = @cobCtr
	where cblScd = @sociedad and cblPer = @periodo and cblFacCod = @codigo and cblNum &lt;&gt; @numcobro
	
        SET @pdte = round(@impFac, 2) - round(@totCob, 2) - round(@impCob, 2)

        update @tablaAuxiliar set pdte = @pdte where periodo = @periodo

        FETCH NEXT FROM cCobros
        INTO @codigo, @periodo, @cobCtr, @impFac, @impCob
    END

    CLOSE cCobros
    DEALLOCATE cCobros

    -- Insert statements for procedure here
	SELECT * from @tablaAuxiliar order by periodo asc</CommandText>
